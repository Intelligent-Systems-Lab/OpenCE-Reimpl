# AppWorld Experiment Configuration Template
# Copy this file to .env and fill in your actual values

# LLM API Configuration
BASE_URL="http://your-server-url:port/v1"
MODEL_NAME="your-model-name"
OPENAI_API_KEY="your-api-key-here"

# AppWorld Server Configuration (optional)
APPWORLD_API_URL="http://localhost:8777"

# Experiment Settings (optional)
TEMPERATURE="0.7"
MAX_INTERACTION_STEPS="5"
MAX_REFINEMENT_ROUNDS="1"
EPOCHS="3"

# Deduplication Settings
# Model used for semantic similarity detection
DEDUPLICATION_MODEL="all-MiniLM-L6-v2"
# Perform deduplication every N samples (0=disabled, >0=every N samples)
# For offline: 0 means no deduplication, N>0 means every N samples
# For online: 0 means no deduplication, N>0 means every N samples
DEDUP_FREQUENCY="5"
